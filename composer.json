{
  "name": "beckson/yii2-comments",
  "description": "Comments module for Yii2 based rmrevin/yii2-comments",
  "keywords": [
    "yii",
    "comment",
    "widget",
    "module"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/becksonq/yii2-comments-1/issues",
    "source": "https://github.com/becksonq/yii2-comments-1"
  },
  "authors": [
    {
      "name": "beckson",
      "email": "becksonq@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",

    "yiisoft/yii2": "2.0.*",
    "rmrevin/yii2-fontawesome": "~2.10"
  },
  "autoload": {
    "psr-4": {
      "beckson\\yii\\module\\comments\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}